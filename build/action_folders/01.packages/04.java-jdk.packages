# java Packages

# install jdk
if [ "${CBF['os_id']}" = 'alpine' ]; then
    declare PKGS='openjdk8'
    package.checkIfInstalled "$PKGS"
    apk add --no-cache $PKGS
fi

# verify it is the correct code
if [ "$JAVA_HOME" != "$( java.home )" ]; then
    echo "java_home:  $( java.home )"
    echo "JAVA_HOME:  $JAVA_HOME"
    exit 1
fi
